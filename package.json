  },
  "homepage": "https://github.com/Kishanjay/css-attribute_classes#readme",
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-sass": "^3.1.0",
    "kss": "^3.0.0-beta.18",
    "michelangelo": "^0.6.1",
    "onchange": "^3.2.1"
