{
  "name": "itl-css-utility_classes",
  "version": "2.0.2",
  "description": "ITL css-utility_classes",
  "main": "gulpfile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "gulp build",
    "watch": "gulp watch",
    "dev": "gulp watch",
    "watch-kss": "onchange './src/scss/**/*.scss' -- npm run build-kss",
    "build-kss": "kss --source ./src/scss/ --destination ./dist/styleguide --css ../css/itl-utility_classes.css --builder node_modules/michelangelo/kss_styleguide/custom-template/ --title 'ITL utility_classes' --homepage ../../README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsLogical/css-utility_classes.git"
  },
  "keywords": [
    "ITL",
    "CSS",
    "prototyping",
    "utility_classes"
  ],
  "author": "Kishan Nirghin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ItsLogical/css-utility_classes/issues"
  },
  "homepage": "https://github.com/ItsLogical/css-utility_classes#readme",
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-sass": "^3.1.0",
    "kss": "^3.0.0-beta.18",
    "michelangelo": "^0.6.1",
    "onchange": "^3.2.1"
  }
}
